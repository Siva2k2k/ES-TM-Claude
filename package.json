{
  "name": "timesheet-management-system",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "cd frontend && npm run dev",
    "build": "cd frontend && npm run build",
    "test": "cd frontend && npm run test",
    "lint": "cd frontend && npm run lint",
    "preview": "cd frontend && npm run preview",
    "install-deps": "cd frontend && npm install && cd ../backend && npm install",
    "clean": "cd frontend && rm -rf dist node_modules && cd ../backend && rm -rf dist node_modules && rm -rf node_modules"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.57.4",
    "axios": "^1.12.2",
    "dotenv": "^17.2.2",
    "exceljs": "^4.4.0",
    "mongodb": "^6.20.0"
  },
  "devDependencies": {
    "@types/exceljs": "^0.5.3",
    "@types/prop-types": "^15.7.15"
  }
}
